{
  "name": "naruhiyo.github.io",
  "version": "1.1.0",
  "description": "A landing page",
  "main": "index.js",
  "scripts": {
    "tcm": "tcm src -p assets/scss/**/*.scss -c -o ./types",
    "predev": "npm run tcm",
    "dev": "webpack-dev-server --config ./webpack.config.js",
    "prebuild": "npm run tcm",
    "build": "rm -rf dist/public/*; NODE_ENV=production webpack",
    "lint": "eslint --ext .ts --ext .tsx src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint --ext .ts --ext .tsx  src/**/*.ts src/**/*.tsx --fix",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./dist",
    "build-storybook": "build-storybook -c .storybook -o public/out",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naruhiyo/naruhiyo.github.io.git"
  },
  "keywords": [
    "Github",
    "Pages"
  ],
  "author": "hiyoko3,narugit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/naruhiyo/naruhiyo.github.io.git/issues"
  },
  "homepage": "https://github.com/naruhiyo/naruhiyo.github.io.git#readme",
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint src/**/*.ts src/**/*.tsx --fix",
      "git add"
    ]
  },
